{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "server/index-prod.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index-prod.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ]
}
